<!DOCTYPE html>
<html>
  <head>
    <title>LuxeSpa</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>

    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">

    </head>

  <body class="brown lighten-5">
      
      <nav class="nav">
      <div class="nav-wrapper brown lighten-3">
      <% if is_logged_in? %>
      <ul>
      
      <li class="right"><%= link_to "Homepage", root_path %></li>
      <li class="right"><%= link_to "Services", services_path %></li>
      <li class="right"><%= link_to "Reviews", reviews_path%></li>
      <li class="right"><%= link_to "My Appointments", appointments_path%></li>
      <li class="right"><%= link_to "Logout", logout_path, method: 'delete' %>
     
      
      </li> 
      </ul>
    <% else %>
    <ul>
      <li class="right"><%= link_to "Homepage", root_path %></li>
      <li class="right"><%= link_to "Login", login_path %></li> 
      <li class="right"><%= link_to "Signup", signup_path %></li>
      <li class="right"><%= link_to "Appointments", appointments_path%></li>
    <% end %>
      </ul>
      </div>
      </nav>
    <div class="wrapper">
    <%= yield %>
  </div>
  </body>
</html>

<!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
