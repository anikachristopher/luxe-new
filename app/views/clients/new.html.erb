<h1>Create an Account</h1>

<%= form_for(@client) do |f|%>

    <%= f.label :first_name %>
    <%= f.text_field :first_name  %>
    <br>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <br>
    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Submit" %>

<% end %>